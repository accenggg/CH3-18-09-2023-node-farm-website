{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/overview", "dest": "/index.js" },
    { "src": "/api", "dest": "/index.js" },
    { "src": "/product", "dest": "/index.js" },
    { "src": "/product\\?id=*", "dest": "/index.js" }
  ]
}
